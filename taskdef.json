{
    "executionRoleArn": "arn:aws:iam::080266302756:role/ecsTaskExecutionRole",
    "containerDefinitions": [
        {
            "name": "ecs-codepipeline-container",
            "image": "080266302756.dkr.ecr.eu-west-3.amazonaws.com/ecs-codedeploy-nginx",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "protocol": "tcp",
                    "containerPort": 80
                }
            ]
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "cpu": "256",
    "memory": "512",
    "family": "ecs-codepipeline-task"
}
